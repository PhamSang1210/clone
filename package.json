{
    "name": "ecomerc",
    "version": "1.0.0",
    "main": "server.js",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "start": "nodemon --inspect server.js",
        "clean": "rm -rf build && mkdir build",
        "build-babel": "babel ./src -d ./build/src",
        "build": "npm run clean && npm run build-babel",
        "production": "npm run build && node ./build/src/server.js",
        "dev": "nodemon --exec ./node_modules/.bin/babel-node server.js"
    },
    "dependencies": {
        "bcrypt": "^5.1.1",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "helmet": "^7.1.0",
        "http-status-codes": "^2.3.0",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "moment-timezone": "^0.5.43",
        "mongoose": "^8.0.1",
        "morgan": "^1.10.0",
        "nodemon": "^3.0.1"
    },
    "devDependencies": {}
}
